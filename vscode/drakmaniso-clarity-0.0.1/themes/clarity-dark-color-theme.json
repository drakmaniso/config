  "tokenColors": [
    {
      "scope": "comment",
      "settings": {
        "foreground": "#b783cf",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "comment punctuation",
      "settings": {
        "foreground": "#b783cf",
        "fontStyle": ""
      }
    },
    {
      "scope": "storage",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "meta.function.definition entity.name.function",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.type",
      "settings": {
        "fontStyle": ""
      }
    },
    {
      "scope": "punctuation",
      "settings": {
        "foreground": "#888888",
      }
    },
    {
      "scope": "meta.brace",
      "settings": {
        "foreground": "#adadad",
        "fontStyle": ""
      }
    },
    {
      "scope": "constant",
      "settings": {}
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#adadad",
        "fontStyle": ""
      }
    },
    {
      "scope": "string punctuation",
      "settings": {
        "foreground": "#adadad",
        "fontStyle": ""
      }
    },
    {
      "scope": "support.type.property-name.json",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "entity.name.tag.yaml",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.inline",
      "settings": {}
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#7db0b9",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.raw.block",
      "settings": {
        "foreground": "#7db0b9"
      }
    },
    {
      "scope": "markup.fenced_code",
      "settings": {
        "foreground": "#7db0b9"
      }
    },
    {
      "scope": "fenced_code.block.language",
      "settings": {
        "foreground": "#9f8d81"
      }
    }
  ]
