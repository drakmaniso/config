{
    "type": "dark",
    "---colors": {
        "editor.background": "#444444",
        "editor.foreground": "#ffffff",
        "sideBar.foreground": "#999999",
        "gitDecoration.addedResourceForeground": "#999999", //"#5fdaf0",
        "gitDecoration.modifiedResourceForeground": "#999999",
        "gitDecoration.untrackedResourceForeground": "#999999",
        "gitDecoration.deletedResourceForeground": "#999999",
        "gitDecoration.renamedResourceForeground": "#999999",
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "comment punctuation"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage",
                "keyword",
                "meta.attribute"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.language"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.annotation",
                "storage.modifier"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "identifier",
                "entity.name"
            ],
            "settings": {}
        },
        {
            "scope": [
                "entity.name.function"
            ],
            "settings": {}
        },
        {
            "scope": [
                "keyword.operator",
                "keyword.operator punctuation"
            ],
            "settings": {
                // "foreground": "#e7e7e7",
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class"
            ],
            "settings": {}
        },
        {
            "scope": "entity.name.function",
            "settings": {}
        },
        {
            "scope": "variable.parameter",
            "settings": {}
        },
        {
            "scope": "punctuation.brackets.curly",
            "settings": {}
        },
        {
            "scope": "punctuation",
            "settings": {}
        },
        {
            "scope": [
                // "constant",
                "string",
                "string punctuation"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "punctuation.support.type.property-name.begin.json",
            "settings": {
                "foreground": "#777",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "punctuation.support.type.property-name.end.json",
            "settings": {
                "foreground": "#777",
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.inline",
            "settings": {}
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.raw.block",
            "settings": {}
        },
        {
            "scope": "markup.fenced_code",
            "settings": {}
        },
        {
            "scope": "fenced_code.block.language",
            "settings": {}
        }
    ]
}