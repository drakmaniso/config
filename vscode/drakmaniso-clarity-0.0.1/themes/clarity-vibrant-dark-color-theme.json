      "settings": {
        "foreground": "#5fdaf0",
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {}
    },
    {
      "scope": "variable.parameter",
      "settings": {}
    },
    {
      "scope": "punctuation",
      "settings": {
        // "foreground": "#777",
      }
    },
    {
      "scope": "constant",
      "settings": {
        "foreground": "#91e77f",
      }
    },
    {
      "scope": [
        "string",
        "string punctuation"
