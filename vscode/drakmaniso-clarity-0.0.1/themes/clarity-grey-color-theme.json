{
    "type": "dark",
    "colors": {
        "editor.background": "#555",
        "editor.foreground": "#000",
        "editorCursor.background": "#888",
        "editorCursor.foreground": "#000",
        // Tabs
        "editorGroupHeader.noTabsBackground": "#444444",
        "editorGroupHeader.border": "#444444",
        "editorGroup.border": "#444444",
        "editorGroup.emptyBackground": "#444444",
        "editorPane.background": "#444444",
        // Activity Bar
        "activityBar.background": "#444444",
        "statusBar.background": "#444444",
        "statusBar.noFolderBackground": "#444444",
        "statusBar.debuggingBackground": "#444444",
        "statusBarItem.remoteBackground": "#444444",
    },
    "tokenColors": [
        {
            "scope": [
                "comment",
                "comment punctuation"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "storage",
                "keyword",
                "meta.attribute"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "variable.language"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": [
                "meta.annotation",
                "storage.modifier"
            ],
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": [
                "identifier",
                "entity.name"
            ],
            "settings": {}
        },
        {
            "scope": [
                "entity.name.function"
            ],
            "settings": {}
        },
        {
            "scope": [
                "keyword.operator",
                "keyword.operator punctuation"
            ],
            "settings": {
                "fontStyle": ""
            }
        },
        {
            "scope": [
                "entity.name.type",
                "entity.name.class"
            ],
            "settings": {}
        },
        {
            "scope": "entity.name.function",
            "settings": {}
        },
        {
            "scope": "variable.parameter",
            "settings": {}
        },
        {
            "scope": "punctuation",
            "settings": {}
        },
        {
            "scope": [
                // "constant",
                "string",
                "string punctuation"
            ],
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "support.type.property-name.json",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "punctuation.support.type.property-name.begin.json",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "punctuation.support.type.property-name.end.json",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.heading",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.inline",
            "settings": {}
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.quote",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.raw.block",
            "settings": {}
        },
        {
            "scope": "markup.fenced_code",
            "settings": {}
        },
        {
            "scope": "fenced_code.block.language",
            "settings": {}
        }
    ]
}